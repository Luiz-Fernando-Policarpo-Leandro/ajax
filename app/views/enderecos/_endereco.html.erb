<div id="<%= dom_id endereco %>">
  <p>
    <strong>Cidade:</strong>
    <%= endereco.cidade.nome%>
  </p>

  <p>
    <strong>Estado:</strong>
    <%= endereco.estado.nome %>
  </p>

  <p>
    <strong>Endereco:</strong>
    <%= endereco.endereco %>
  </p>

  <p>
    <strong>Bairro:</strong>
    <%= endereco.bairro %>
  </p>

  <p>
    <strong>Cep:</strong>
    <%= endereco.cep %>
  </p>

  <p>
    <strong>Numero:</strong>
    <%= endereco.numero %>
  </p>

</div>
